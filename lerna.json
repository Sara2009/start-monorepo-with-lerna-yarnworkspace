{
  "version": "0.0.0",
  "npmClient": "yarn",
  "command": {
    "publish": {
      "ignoreChanges": [
        "ignored-file",
        "**/__tests__/**",
        "LICENSE"
      ],
      "message": "chore(release): publish %s",
      "registry": "https://registry.npmjs.org/",
      "npmClientArgs": [
        "from-git"
      ]
    },
    "bootstrap": {
      "npmClientArgs": [
        "--no-package-lock"
      ]
    }
  },
  "useWorkspaces": true
}
